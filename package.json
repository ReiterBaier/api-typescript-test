{
  "name": "busca-cep",
  "version": "1.0.0",
  "description": "API para consumo de dados referente a cadastros de CEP e cadastros de CIDADE",
  "main": "index.js",
  "repository": "https://github.com/ReiterBaier/fsis.cep.git",
  "author": "Reiter Baier<joao.victor@sisplansistemas.com.br>",
  "license": "MIT",
  "scripts": {
    "dev": "set NODE_ENV=development&& tsnd -r tsconfig-paths/register --respawn --transpile-only --ignore-watch node_modules --no-notify ./src/index.ts",
    "test": "set NODE_ENV=test&& jest --i --no-cache --verbose --runInBand --bail",
    "typeorm": "ts-node-dev  node_modules/typeorm/cli.js",
    "build:babel": "set BABEL_ENV=build&& set NODE_ENV=production&& babel src --extensions \".ts\" --out-dir ./fsis/dist --source-maps --copy-files --no-copy-ignored --delete-dir-on-start --no-comments",
    "install_service": "set NODE_ENV=development&& tsnd -r tsconfig-paths/register --respawn --transpile-only --ignore-watch node_modules --no-notify ./service.ts",
    "build": "yarn build:clean&&yarn build:dist",
    "build:clean": "rimraf ./dist",
    "build:dist": "tsc --build tsconfig.build.json"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "module-alias": "^2.2.2",
    "multer": "^1.4.3",
    "mysql": "^2.18.1",
    "node-windows": "1.0.0-beta.5",
    "pg": "^8.7.3",
    "reflect-metadata": "^0.1.13",
    "routing-controllers": "^0.9.0",
    "typeorm-routing-controllers-extensions": "^0.2.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.15.0",
    "@babel/node": "^7.14.9",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-decorators": "^7.14.5",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-typescript": "^7.15.0",
    "@babel/register": "^7.15.3",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.3",
    "@types/module-alias": "^2.0.1",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.11.10",
    "@types/node-windows": "^0.1.2",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-transform-decorators": "^6.24.1",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "eslint": "^8.3.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.3.1",
    "prettier": "^2.4.1",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.7",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.12.0",
    "typeorm": "^0.3.1",
    "typescript": "^4.5.2"
  }
}
